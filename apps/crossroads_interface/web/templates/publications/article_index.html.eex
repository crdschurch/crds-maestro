<div class="row">
  <%= for post <- assigns.posts do %>
    <div class="col-md-4">
      <div class="card">
        <div class="card-block">
          <h4 class="card-title"><%= post["title"] %></h4>
          <p class="card-text"><%= post["author"] %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
